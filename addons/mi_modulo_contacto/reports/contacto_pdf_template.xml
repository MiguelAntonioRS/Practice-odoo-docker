<odoo>
    <template id="contacto_pdf_template">
        <t t-call="web.basic_layout">
            <div class="page">
                <h2>Información de Contacto</h2>
                <p><strong>Nombre:</strong> <t t-esc="object.name"/></p>
                <p><strong>Email:</strong> <t t-esc="object.email"/></p>
                <p><strong>Teléfono:</strong> <t t-esc="object.phone"/></p>
                <p><strong>Dirección:</strong> <t t-esc="object.street or 'No disponible'"/></p>
            </div>
        </t>
    </template>
</odoo>
